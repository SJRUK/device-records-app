<h1>Search Database</h1>

<h2>Search results</h2>

<ul class="devices">
    <!-- Loop through devices -->
    <% for(var i = 0; i < devices.length; i++) {%>
    <li class="device">
      <!-- Output assetTag from the iterated quote object -->
      <span><%= devices[i].assetTag %></span>:
      <!-- Output name from the iterated quote object -->
      <span><%= devices[i].name %></span>:
      <!-- Output organisation from the iterated quote object -->
      <span><%= devices[i].organisation %></span>:
      <!-- Output line manager from the iterated quote object -->
      <span><%= devices[i].lineManager %></span>:
      <!-- Output date from the iterated quote object -->
      <span><%= devices[i].dateRequired %></span>
      </li>
      <% } %>
    </ul>


    <div>
      <h2>Search Devices</h2>
      <p>Search the database using the Asset Number</p>
      <input type="text" placeholder="Asset Number" id="searchInput">  
      <button id="search-button">Search</button>
        
    </div>

    <div id="resultsContainer">     </div>
    